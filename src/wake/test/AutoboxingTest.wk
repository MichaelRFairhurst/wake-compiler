		Asserts.that(5.0.ceil())Equals(5);
		Asserts.that(-5.ceil())Equals(-5);
		Asserts.that(-5.1.ceil())Equals(-5);
		Asserts.that(-5.5.ceil())Equals(-5);
		Asserts.that(-5.9.ceil())Equals(-5);
	}

	testRound(Asserts) {
