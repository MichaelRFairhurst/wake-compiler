		Asserts.that(5.0.round())Equals(5);
		Asserts.that(-5.round())Equals(-5);
		Asserts.that(-5.1.round())Equals(-5);
		Asserts.that(-5.5.round())Equals(-5);
		Asserts.that(-5.51.round())Equals(-6);
		Asserts.that(-5.9.round())Equals(-6);
	}

